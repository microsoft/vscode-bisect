{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "out": true,
        ".vscode-test": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "out": true
    },
    "typescript.tsc.autoDetect": "off",
    "git.branchProtection": [
        "main"
    ],
    "git.branchProtectionPrompt": "alwaysCommitToNewBranch",
    "git.branchRandomName.enable": true,
    "git.pullBeforeCheckout": true,
    "chat.instructionsFilesLocations": {
        "src": true,
        ".vscode": true
    },
    "chat.modeFilesLocations": {
        ".vscode": true
    },
    "chat.promptFilesLocations": {
        "src": true,
        ".vscode": true
    },
    "[instructions]": {
        "editor.quickSuggestions": {
			"comments": "off",
            "strings": "off",
            "other": "off"
		},
        "editor.wordWrap": "bounded"
    },
    "[prompt]": {
        "editor.quickSuggestions": {
			"comments": "off",
            "strings": "off",
            "other": "off"
		},
        "editor.wordWrap": "bounded"
    },
    "[chatmode]": {
        "editor.quickSuggestions": {
			"comments": "off",
            "strings": "off",
            "other": "off"
		},
        "editor.wordWrap": "bounded"
    }
}