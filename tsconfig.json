{
  "compilerOptions": {
    "target": "es2022",
		"module": "nodenext",
		"lib": ["es2022"],
    "rootDir": "./src",
    "outDir": "./out",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
